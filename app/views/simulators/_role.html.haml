%table
  %tr
    %th= role
    %th= link_to 'Remove Role', url_for(:action => :remove_role, :role => role.name), :method => :post
  =render :partial => "strategy", :collection => role.strategies, :locals => {role: role}
  =form_tag url_for(:action=> "add_strategy", :id => role.role_owner.id, :role => role.name), :method=>:post do
    %tr
      %td= text_field_tag "#{role}_strategy".to_sym
      %td= submit_tag "Add Strategy", {:id => role.name}